{
  "name": "ytake/hack-ddd-repository-sample",
  "type": "project",
  "minimum-stability": "stable",
  "description": "#phpconsen example",
  "require": {
    "hhvm": "^4.62",
    "hhvm/hsl": "^4.0",
    "hhvm/hhvm-autoload": "^3.0",
    "hhvm/hsl-experimental": "^4.50"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0",
    "facebook/fbexpect": "^2.7",
    "hhvm/hhast": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/"
    }
  },
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
